<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.heremaps.app">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" /> <!-- To allow starting foreground services on Android P+ -->
    <!-- https://developer.android.com/preview/behavior-changes#fg-svc -->
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />


    <application
      android:name=".MainApplication"
      android:label="@string/app_name"
      android:icon="@mipmap/ic_launcher"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:allowBackup="false"
      android:theme="@style/AppTheme">
      <activity
        android:name=".MainActivity"
        android:label="@string/app_name"
        android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
        android:windowSoftInputMode="adjustResize">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
      </activity>
      <activity android:name="com.facebook.react.devsupport.DevSettingsActivity" />
        <activity android:name=".heremap.SplashActivity">
        </activity>
        <activity android:name=".heremap.MainActivity">

        </activity>
        <!--
Developers should put application credentials here.To obtain them, please register the application
         at https://developer.here.com/develop/mobile-sdks
        -->
        <meta-data
            android:name="com.here.android.maps.appid"
            android:value="2LHlGxryBICkltpvbEzW" />
        <meta-data
            android:name="com.here.android.maps.apptoken"
            android:value="GxPGvWflvL8zqTh865nyqg" />
        <meta-data
            android:name="com.here.android.maps.license.key"
            android:value="" />
        <!--
Developers should always provide custom values for each of {YOUR_LABEL_NAME} and {YOUR_INTENT_NAME}.
        Do not reuse HERE Mobile SDK defaults.
        -->
        <meta-data
            android:name="INTENT_NAME"
            android:value="ILEAF_MAP_INTENT" />

        <service
            android:name="com.here.android.mpa.service.MapService"
            android:exported="false"
            android:label="ILEAF_MAP_SERVICE">
            <intent-filter>
                <action android:name="ILEAF_MAP_INTENT" />
            </intent-filter>
        </service>
        <service android:name=".heremap.ForegroundService" />
    </application>

</manifest>
